{
    "version": 2,
    "builds": [
      {
        "src": "dist/main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "dist/main.js"
      },
      {
        "src": "/(.*)",
        "status": 404,
        "dest": "/404.html"
      }
    ]
  }
  